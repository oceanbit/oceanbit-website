---
interface Props {
  stroke?: string;
  className?: string | undefined;
}

const { stroke = "white", className = "" } = Astro.props;
---

<svg
  width="20"
  height="20"
  viewBox="0 0 20 20"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
  class={className}
>
  <g id="Group">
    <path
      id="Vector"
      d="M8.33337 13.3333L11.6667 9.99996L8.33337 6.66663"
      stroke={stroke}
      stroke-width="1.5"
      stroke-linecap="round"
      stroke-linejoin="round"></path>
  </g>
</svg>
