---
interface Props {
  stroke?: string;
  className?: string;
}

const { stroke = "#697586", className } = Astro.props;
---

<svg
  class={className}
  width="24"
  height="24"
  viewBox="0 0 24 24"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
>
  <g id="Base / x">
    <path
      id="Icon"
      d="M17 7L7 17M7 7L17 17"
      stroke={stroke}
      stroke-width="2"
      stroke-linecap="round"
      stroke-linejoin="round"></path>
  </g>
</svg>
